<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
 <servlet>
     <servlet-name>MyFilterServlet</servlet-name>
     <servlet-class>MyFilterServlet</servlet-class>
</servlet> 
 <servlet-mapping>
         <servlet-name>MyFilterServlet</servlet-name>
         <url-pattern>/myst</url-pattern>
  </servlet-mapping>
 <servlet>
     <servlet-name>MyFilterServlet1</servlet-name>
     <servlet-class>MyFilterServlet1</servlet-class>
</servlet> 
 <servlet-mapping>
         <servlet-name>MyFilterServlet1</servlet-name>
         <url-pattern>/myst1</url-pattern>
  </servlet-mapping>  
<filter>
     <filter-name>MyFilter</filter-name>
     <filter-class>MyFilter</filter-class>
</filter> 
 <filter-mapping>
         <filter-name>MyFilter</filter-name>
         <url-pattern>/myst</url-pattern>
  </filter-mapping>
 <filter>
     <filter-name>MyFilter1</filter-name>
     <filter-class>MyFilter</filter-class>
</filter> 
 <filter-mapping>
         <filter-name>MyFilter1</filter-name>
         <url-pattern>/myst1</url-pattern>
  </filter-mapping>
</web-app>